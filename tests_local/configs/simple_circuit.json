{
    "experiment": {
        "purpose": "tests",
        "test_mode": false,
        "no_visualisations": false,
        "no_numerical": false
    },
    "interaction_simulator": {
        "name": "IntaRNA",
        "postprocess": true
    },
    "molecular_params": "src/utils/common/configs/RNA_circuit/molecular_params.json",
    "interaction_file_keyword": ["eqconstants", "binding_rates_dissociation"],
    "mutations": {
        "algorithm": "random",
        "mutation_counts": 0,
        "mutation_nums_within_sequence": [1],
        "mutation_nums_per_position": 1,
        "seed": 3,
        "concurrent_species_to_mutate": "single_species_at_a_time"
    },
    "filters": {
        "min_num_interacting": null,
        "max_self_interacting": null,
        "max_total": null
    },
    "signal": {
        "inputs": ["RNA_0"],
        "outputs": ["RNA_1"],
        "function_name": "step_function_integrated",
        "function_kwargs": {
            "impulse_center": 5, 
            "target": 2
        }
    },
    "simulation": {
        "dt": 0.1,
        "t0": 0,
        "t1": 100,
        "tmax": 20000,
        "solver": "diffrax",
        "use_batch_mutations": true,
        "batch_size": 100,
        "max_circuits": 1000,
        "device": "cpu",
        "threshold_steady_states": 0.1
    },
    "system_type": "RNA"
}